<launch>
  <node name="orb_slam2_stereo" pkg="orb_slam2_ros"
      type="orb_slam2_ros_stereo" args="
          $(find orb_slam2_ros)/orb_slam2/Vocabulary/ORBvoc.txt
          $(find orb_slam2_ros)/orb_slam2/config/RealSenseR200Stereo.yaml"
      respawn="true">
       <remap from="image_left/image_color_rect" to="/md_rov/stereocam/left/image_raw" />
       <remap from="image_right/image_color_rect" to="/md_rov/stereocam/right/image_raw" />

       <param name="publish_pointcloud" type="bool" value="true" />
       <param name="publish_pose" type="bool" value="true" />
       <param name="pointcloud_frame_id" type="string" value="camera_depth_optical_frame" />
       <param name="camera_frame_id" type="string" value="camera_link" />
  </node>
</launch>
